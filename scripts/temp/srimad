<div class = 'custom_display_odd'>
      <div >
                                                            <div class="views-field views-field-field-scsri"                       <p align = 'center'><font color = '#2c44bd' size = '4px'><b>Sanskrit Commentary By Sri Sridhara Swami</b></font></p><p align = 'justify'><font size = '3px'> <b>।।1.1।।</b> इह खलु सकललोकहितावतारः सकलवन्दितचरणः परमकारुणिको भगवान् देवकीनन्दनस्तत्त्वाज्ञानविजृम्भितशोकमोहविभ्रंशितविवेकतया निजधर्मत्यागपरधर्माभिसंधिपरमर्जुनं धर्मज्ञानरहस्योपदेशप्लवेन तस्माच्छोकमोहसागरादुद्दधार। तमेव भगवदुपदिष्टमर्थं कृष्णद्वैपायनः सप्तभिः श्लोकशतैरुपनिबबन्ध। तत्र च प्रायशः श्रीकृष्णमुखनिःसृतानेव श्लोकानलिखत् कांश्चित्तत्संगतये स्वयं व्यरचयत्। यथोक्तं गीतामाहात्म्ये  गीता सुगीता कर्तव्या किमन्यैः शास्त्रविस्तरैः। या स्वयं पद्मनाभस्य मुखपद्माद्विनिःसृता।। इति। तत्र तावद्धर्मक्षेत्र इत्यादिना <br><br>विषीदन्निदमब्रवीदित्यन्तेन ग्रन्थेन श्रीकृष्णार्जुनसंवादप्रस्तावाय कथा निरूप्यते <b>  धर्मक्षेत्र इति।</b> भो संजय धर्मभूमौ कुरुक्षेत्रे मत्पुत्राः पाण्डुपुत्राश्च युयुत्सवो योद्धुमिच्छन्तः समवेता मिलिताः सन्तः किं कृतवन्तः।<br><br></font></p>
          </div>
                        </div>
    </div>



</div>

<script type="text/javascript">
    function playAudio(){
        if(document.getElementById('slkaudio') != null)
            document.getElementById('slkaudio').play();
        else if (document.getElementById('purohitAudio') != null)
            document.getElementById('purohitAudio').play();
        else if (document.getElementById('tejAudio') != null)
            document.getElementById('tejAudio').play();
    }
    if(document.getElementById("edit-enable-autoplay").checked || document.getElementById("edit-enable-contplay").checked)
    	playAudio();
    function playNext(id, contPlay = false){
        switch(id){
            case 'slkaudio':
                if (document.getElementById('purohitAudio') != null)
                    document.getElementById('purohitAudio').play();
                else if (document.getElementById('tejAudio') != null)
                    document.getElementById('tejAudio').play();
                else if(contPlay)
                    nextAudio();
		break;
            case 'purohitAudio':
                if (document.getElementById('tejAudio') != null)
                    document.getElementById('tejAudio').play();
                else if(contPlay)
                    nextAudio();
                break;
            case 'tejAudio':
                if(contPlay)
                    nextAudio();
                break;
        }
    }

    function nextAudio(){
        var regex = /[?&]([^=#]+)=([^&#]*)/g;
        var url = window.location.href;
        var params = {};
        var match;
        var args = '&';
        while(match = regex.exec(url)) {
            params[match[1]] = match[2];
        }
        // console.log(params);
        var lang = document.getElementById('edit-language').value;
        var chapter = parseInt(document.getElementById('edit-field-chapter-value').value);
        var sloka = parseInt(document.getElementById('edit-field-nsutra-value').value);
        var next_sloka;
        var max_sloka;
        if(chapter == 1) max_sloka = 47;
        else if (chapter == 2) max_sloka = 72;
        else if (chapter == 3) max_sloka = 43;
        else if (chapter == 4) max_sloka = 42;
        else if (chapter == 5) max_sloka = 29;
        else if (chapter == 6) max_sloka = 47;
        else if (chapter == 7) max_sloka = 30;
        else if (chapter == 8) max_sloka = 28;
        else if (chapter == 9) max_sloka = 34;
        else if (chapter == 10) max_sloka = 42;
        else if (chapter == 11) max_sloka = 55;
        else if (chapter == 12) max_sloka = 20;
        else if (chapter == 13) max_sloka = 35;
        else if (chapter == 14) max_sloka = 27;
        else if (chapter == 15) max_sloka = 20;
        else if (chapter == 16) max_sloka = 24;
        else if (chapter == 17) max_sloka = 28;
        else if (chapter == 18) max_sloka = 78;
        switch(chapter){
            case 1:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 2;
                    next_sloka = 1;
                }
                break;
            case 2:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 3;
                    next_sloka = 1;
                }
                break;
            case 3:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 4;
                    next_sloka = 1;
                }
                break;
            case 4:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 5;
                    next_sloka = 1;
                }
                break;
            case 5:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 6;
                    next_sloka = 1;
                }
                break;
            case 6:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 7;
                    next_sloka = 1;
                }
                break;
            case 7:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 8;
                    next_sloka = 1;
                }
                break;
            case 8:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 9;
                    next_sloka = 1;
                }
                break;
            case 9:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 10;
                    next_sloka = 1;
                }
                break;
            case 10:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 11;
                    next_sloka = 1;
                }
                break;
            case 11:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 12;
                    next_sloka = 1;
                }
                break;
            case 12:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 13;
                    next_sloka = 1;
                }
                break;
            case 13:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 14;
                    next_sloka = 1;
                }
                break;
            case 14:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 15;
                    next_sloka = 1;
                }
                break;
            case 15:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 16;
                    next_sloka = 1;
                }
                break;
            case 16:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 17;
                    next_sloka = 1;
                }
                break;
            case 17:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 18;
                    next_sloka = 1;
                }
                break;
            case 18:
                if(sloka >= 1 && sloka < max_sloka){
                    next_sloka = sloka + 1;
                }
                else {
                    chapter = 1;
                    next_sloka = 1;
                }
                break;
        }
        for(parameter in params){
            if(parameter != 'field_chapter_value' && parameter != 'field_nsutra_value' && parameter != 'choose'){
                if(args != '&') args += '&'+parameter+"="+params[parameter];
                else args += parameter+"="+params[parameter];
            }
        }
        window.location.href = "srimad?field_chapter_value="+chapter+"&field_nsutra_value="+next_sloka+args;
    }
</script>
    </div>
  
  
